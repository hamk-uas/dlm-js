{
  "name": "dlm-js",
  "version": "0.1.0",
  "description": "JavaScript re-implementation of the MATLAB dlm library using jax-js",
  "type": "module",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@jax-js/jax": "git+https://github.com/olli4/jax-js.git#feat/scan-v2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@jax-js/jax"
    ]
  },
  "devDependencies": {
    "@hamk-uas/eslint-plugin-jax-js": "github:hamk-uas/eslint-plugin-jax-js#v0.1.1",
    "@types/node": "^25.0.10",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.6.1"
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:octave": "octave-cli --no-gui tests/octave/niledemo.m",
    "test:node": "vitest run tests/niledemo.test.ts",
    "test": "npm run test:octave && npm run test:node",
    "prepublishOnly": "npm run build && npm run test"
  },
  "main": "./dist/dlm-js.cjs.js",
  "module": "./dist/dlm-js.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dlm-js.es.js",
      "require": "./dist/dlm-js.cjs.js"
    }
  }
}
